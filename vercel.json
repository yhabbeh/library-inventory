{
  "builds": [
    {
      "src": "works.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/works.html",
      "dest": "/works.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}